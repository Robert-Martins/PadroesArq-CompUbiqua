{
  "expo": {
    "name": "agende-saude-app",
    "slug": "agende-saude-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "agende-saude-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.agendesaudeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Thin.ttf",
            "./assets/fonts/Inter-ThinItalic.ttf",
            "./assets/fonts/Inter-ExtraLight.ttf",
            "./assets/fonts/Inter-ExtraLightItalic.ttf",
            "./assets/fonts/Inter-Light.ttf",
            "./assets/fonts/Inter-LightItalic.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-Italic.ttf",
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-MediumItalic.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Inter-SemiBoldItalic.ttf",
            "./assets/fonts/Inter-Bold.ttf",
            "./assets/fonts/Inter-BoldItalic.ttf"
          ]
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Permita que o Agende Saúde use o Face ID para autenticar você"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita que o Agende Saúde acesse sua localização enquanto estiver usando o app"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita que o Agende Saúde acesse suas fotos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f25ef69-e08d-4203-a34f-193939bbf997"
      }
    }
  }
}
